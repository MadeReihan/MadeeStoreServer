<table id="example2" class="table table-bordered table-hover">
    <thead>
    <tr>
      <th>No</th>
      <th>Avatar</th>
      <th>Username</th>
      <th>Phone Number</th>
      <th>Status</th>
      <th>Role</th>
    </tr>
    </thead>
    <tbody>
      <% for(let i = 0; i<customer.length; i++){ %>
        <tr>
          <td><%= i+1 %></td>
          <td>
            <img src="/uploads/<%=customer[i].avatar%>" width="50px" height="50px">
          </td>
          <td>
            <%= customer[i].username%>
          </td>
          <td>
            <%= customer[i].phoneNumber%>
          </td>
          <td>
            <%= customer[i].status%>
          </td>
          <td>
            <%= customer[i].role%>
          </td>
        </tr>
      <%} %>
    </tbody>
    <tfoot>
    <tr>
        <th>No</th>
        <th>Avatar</th>
        <th>Username</th>
        <th>Phone Number</th>
        <th>Status</th>
        <th>Role</th>
    </tr>
    </tfoot>
  </table>